<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <stroke
        android:width="1dp"
        android:color="#000000">
    </stroke>

    <solid
        android:color="#FFFFFF">
    </solid>

    <size
        android:width="40dp"
        android:height="40dp">
    </size>

    <padding
        android:bottom="5dp"
        android:top="5dp"
        android:left="5dp"
        android:right="5dp">
    </padding>
</shape>